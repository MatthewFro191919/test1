import deepend.dialogue.DialogueSoundData;

var expungedSounds:DialogueSoundData;

function onDialogueStarted() {
	trace('im born anew');
	expungedSounds = new DialogueSoundData([for (i in 1...9) '$i'], 'expunged/exp', [-.05, .05]);
}

function onSoundPlay(char, pos) {
	if (pos >= 97 && box.curCharacter == null) {
		expungedSounds.playSound(box.dialogueSound);
	}
}