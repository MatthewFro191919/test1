    {
      "character": "girlfriend",
      "position": "right",
      "emotion": "somber",
      "text": "Really? Even though you didn't want to?",
      "font": null,
      "boxEmotion": "regular",
      "sound": null,
      "still": null,
      "event": null,
      "background": null,
      "song": null
    },
    {
      "character": "tristan",
      "position": "left",
      "emotion": "grabbing",
      "text": "Mhm! I feel bad making you do it.",
      "font": null,
      "boxEmotion": "regular",
      "sound": null,
      "still": null,
      "event": null,
      "background": null,
      "song": null
    },
    {
      "character": "girlfriend",
      "position": "right",
      "emotion": "neutral_happy",
