      "background": null,
      "song": null
    },
    {
      "character": "tristan",
      "position": "left",
      "emotion": "annoyed",
      "text": "Seriously? I knew this would be too weird.",
      "font": null,
      "boxEmotion": "regular",
      "sound": null,
      "still": null,
      "event": null,
      "background": null,
      "song": null
